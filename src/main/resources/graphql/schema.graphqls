enum Dimension{
    BY_MONTH
    BY_DAY
    BY_LINE
}
type IndicatorValue {
     name: String
     value: Int
     error: String
}

type IndicatorsGroupPart {
     part: Int
     indicators: [IndicatorValue]
}

type IndicatorsGroup {
	dimension: Dimension,
	indicatorsGroupParts: [IndicatorsGroupPart]
}

type IndicatorFormula {
	name: String,
	expression: String
}

type Query {
	group(dimension: Dimension, indicatorNames: [String], begin: String, end: String, id_user: Int): IndicatorsGroup
}
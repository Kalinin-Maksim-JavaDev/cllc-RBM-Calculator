type IndicatorValue {
     name: String
     value: Int
}

type IndicatorsGroupPart {
     part: Int
     indicators: [IndicatorValue]
}

type IndicatorsGroup {
	name: String,
	indicatorsGroupParts: [IndicatorsGroupPart]
	subGroups: [IndicatorsGroup]
}

type Query {
	group(name: String, indicatorNames: [String], begin: String, end: String, id_user: Int): IndicatorsGroup
}
